{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "测试调试功能",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "program": "${workspaceFolder}/src/bin/pm.ts",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "console": "integratedTerminal"
        }
    ]
}